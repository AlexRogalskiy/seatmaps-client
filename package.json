{
  "name": "ticket-evolution-seatmaps",
  "version": "0.0.1",
  "description": "interactive seatmaps",
  "repository": {
    "url": "git@github.com:ticketevolution/seatmaps-client.git",
    "type": "git"
  },
  "main": "build/tevomaps.js",
  "author": "BRAD LARSON",
  "license": "UNLICENSED",
  "engines": {
    "npm": ">=8"
  },
  "scripts": {
    "build:prod": "webpack --mode=production --config internals/webpack/webpack.prod.babel.js --color",
    "start": "webpack-dev-server --config internals/webpack/webpack.dev.babel.js --hot",
    "start:prod": "webpack-dev-server --config internals/webpack/webpack.prod.babel.js",
    "pretest": "flow",
    "test": "cross-env NODE_ENV=test jest --coverage --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --no-cache --watchAll",
    "analyze": "webpack --config internals/webpack/webpack.prod.babel.js --profile --json > stats.json"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": ".*\\.test\\.js$"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "react-inlinesvg": "^0.8.2",
    "react-toggled": "1.2.6",
    "react-transition-group": "2.3.1",
    "spin.js": "3.1.0",
    "svg-pan-zoom": "3.5.3",
    "unfetch": "3.0.0"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^3.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-preact": "1.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "4.1.2",
    "compression-webpack-plugin": "1.1.11",
    "cross-env": "5.1.5",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "exports-loader": "0.7.0",
    "expose-loader": "^0.7.5",
    "file-loader": "1.1.11",
    "flow-bin": "0.72.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.2.0",
    "imports-loader": "0.8.0",
    "jest-cli": "23.0.0-alpha.0",
    "localStorage": "1.0.3",
    "null-loader": "0.1.1",
    "preact-jsx-chai": "2.2.1",
    "prettier": "1.12.1",
    "shelljs": "0.8.2",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "^3.1.10"
  }
}