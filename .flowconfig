[ignore]
.*/node_modules/.*[^e]\.json$
.*/build.*
.*/*.test.js

[include]

[libs]
./src/flowtype

[options]
suppress_comment=.*\\$FlowFixMe
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json

[lints]
untyped-type-import=error
untyped-import=warn
unclear-type=warn
unsafe-getters-setters=error
